{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Projectes",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "strategies": {
      "via": "projects",
      "collection": "strategy",
      "dominant": true
    },
    "project_scope": {
      "model": "project-scope"
    },
    "leader": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "intercooperations": {
      "via": "projectes",
      "collection": "contacts",
      "dominant": true
    },
    "incomes": {
      "type": "component",
      "repeatable": true,
      "component": "budget-line.budget-line"
    },
    "estimated_hours": {
      "type": "component",
      "repeatable": true,
      "component": "hours.hours"
    },
    "total_estimated_hours": {
      "type": "decimal"
    },
    "total_real_hours": {
      "type": "decimal"
    },
    "total_expenses": {
      "type": "decimal"
    },
    "total_incomes": {
      "type": "decimal"
    },
    "total_expenses_hours": {
      "type": "decimal"
    },
    "balance": {
      "type": "decimal"
    },
    "total_estimated_expenses": {
      "type": "decimal"
    },
    "estimated_balance": {
      "type": "decimal",
      "default": 0
    },
    "incomes_expenses": {
      "type": "decimal"
    },
    "project_state": {
      "model": "project-state"
    },
    "clients": {
      "via": "projects",
      "collection": "contacts",
      "dominant": true
    },
    "activities": {
      "via": "project",
      "collection": "activity"
    },
    "invoice_hours_price": {
      "type": "decimal",
      "default": 0
    },
    "expenses": {
      "type": "component",
      "repeatable": true,
      "component": "expense.expense"
    },
    "emitted_invoices": {
      "via": "project",
      "collection": "emitted-invoice"
    },
    "received_invoices": {
      "via": "project",
      "collection": "received-invoice"
    },
    "tickets": {
      "collection": "ticket",
      "via": "project"
    },
    "diets": {
      "collection": "diet",
      "via": "project"
    },
    "emitted_grants": {
      "collection": "emitted-grant",
      "via": "project"
    },
    "received_grants": {
      "via": "project",
      "collection": "received-grant"
    },
    "activity_types": {
      "via": "project",
      "collection": "activity-type"
    },
    "quotes": {
      "collection": "quote",
      "via": "project"
    },
    "project_type": {
      "model": "project-type"
    },
    "total_real_incomes": {
      "type": "decimal"
    },
    "total_real_expenses": {
      "type": "decimal"
    },
    "total_real_incomes_expenses": {
      "type": "decimal"
    },
    "date_start": {
      "type": "date"
    },
    "date_end": {
      "type": "date"
    },
    "structural_expenses": {
      "type": "boolean"
    },
    "structural_expenses_pct": {
      "type": "decimal"
    },
    "total_estimated_hours_price": {
      "type": "decimal"
    },
    "total_real_hours_price": {
      "type": "decimal"
    },
    "phases": {
      "type": "component",
      "repeatable": true,
      "component": "project-phase.project-phase"
    },
    "ingressos_rebuts": {
      "via": "projecte",
      "collection": "received-income"
    },
    "default_dedication_type": {
      "model": "dedication-type"
    }
  }
}
